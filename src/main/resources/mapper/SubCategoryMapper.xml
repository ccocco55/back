<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.youeatieat.mapper.SubCategoryMapper">
    <select id="selectByMainCategory">
        select sub_category_name
        from tbl_sub_category
        where main_category_id = #{id}
    </select>
</mapper>